<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="525.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.ReclamationClient.PageReclamationsController">
    <children>
      <AnchorPane prefHeight="150.0" prefWidth="600.0" style="-fx-background-color: #566573;">
         <children>
            
         </children>
      </AnchorPane>
      <Button fx:id="backButton" layoutX="64.0" layoutY="460.0" mnemonicParsing="false" onAction="#onBack" prefHeight="26.0" prefWidth="102.0" style="-fx-background-color: #566573;" text="Back" />
      <Button fx:id="addReclamation" layoutX="417.0" layoutY="460.0" mnemonicParsing="false" onAction="#onAddReclamation" prefHeight="26.0" prefWidth="102.0" style="-fx-background-color: #566573;" text="Add" />
      <TableView fx:id="reclamationView" layoutX="49.0" layoutY="205.0" onMouseClicked="#onLigne" prefHeight="200.0" prefWidth="502.0" style="-fx-background-color: transparent;">
        <columns>
          <TableColumn fx:id="champcontenu" prefWidth="250.0" text="Content" />
          <TableColumn fx:id="champState" minWidth="0.0" prefWidth="250.0" text="State" />
            <TableColumn fx:id="idcolumn" prefWidth="75.0" text="Column X" visible="false" />
        </columns>
      </TableView>
      <Button fx:id="btnDelete" layoutX="232.0" layoutY="405.0" mnemonicParsing="false" onAction="#onDelete" prefHeight="26.0" prefWidth="102.0" style="-fx-background-color: #566573;" text="Delete" />
      <Label fx:id="errorLabel" layoutX="212.0" layoutY="443.0" textFill="RED">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
