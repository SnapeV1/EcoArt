<AnchorPane id="AnchorPane" prefHeight="700.0" prefWidth="700.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="gui.EspacePersonel.Update.UpdateController">
   <children>
      <AnchorPane prefHeight="700.0" prefWidth="300.0" style="-fx-background-color: #566573;">
         <children>
            <Button fx:id="confirm" layoutX="95.0" layoutY="630.0" mnemonicParsing="false" onAction="#onConfirm" prefHeight="26.0" prefWidth="96.0" style="-fx-background-color: transparent; -fx-border-color: white;" text="Confirm" />
            <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="43.0" layoutY="277.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../../../../../nnn.png" />
               </image>
            </ImageView>
            <ImageView fx:id="entryImage" fitHeight="150.0" fitWidth="150.0" layoutX="35.0" layoutY="25.0" onMouseClicked="#onImage" pickOnBounds="true" preserveRatio="true" />
            <Button fx:id="btnUpdatePass" layoutX="73.0" layoutY="411.0" mnemonicParsing="false" onAction="#onUpdatePassw" prefHeight="27.0" prefWidth="126.0" style="-fx-background-color: transparent; -fx-border-color: white;" text="Update Password" />
         </children>
      </AnchorPane>
      <TextField fx:id="usernameLabel" layoutX="334.0" layoutY="58.0" promptText="Username" style="-fx-background-color: transparent; -fx-border-color: #566573; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="nameLabel" layoutX="334.0" layoutY="126.0" promptText="Name" style="-fx-background-color: transparent; -fx-border-color: #566573; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="lastNameLabel" layoutX="334.0" layoutY="198.0" promptText="Lastname" style="-fx-background-color: transparent; -fx-border-color: #566573; -fx-border-width: 0px 0px 2px 0px;" />
      <DatePicker fx:id="birthdayLabel" layoutX="321.0" layoutY="281.0" onAction="#onCalendar" promptText="BirthDay" style="-fx-background-color: transparent; -fx-border-color: #566573; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="emailLabel" layoutX="334.0" layoutY="350.0" promptText="Email" style="-fx-background-color: transparent; -fx-border-color: #566573; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="ageLabel" layoutX="334.0" layoutY="432.0" promptText="Age" style="-fx-background-color: transparent; -fx-border-color: #566573; -fx-border-width: 0px 0px 2px 0px;" />
      <TextField fx:id="cinLabel" layoutX="334.0" layoutY="510.0" promptText="CIN" style="-fx-background-color: transparent; -fx-border-color: #566573; -fx-border-width: 0px 0px 2px 0px;" />
      <Label fx:id="typeLabel" layoutX="350.0" layoutY="596.0" text="Type">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label fx:id="mdpError" layoutX="317.0" layoutY="639.0" textFill="RED">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
